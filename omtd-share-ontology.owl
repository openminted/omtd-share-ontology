        <rdfs:comment xml:lang="en">The task/process of assigning documents into classes or categories</rdfs:comment>
        <rdfs:label xml:lang="en">Text categorization</rdfs:label>
        <skos:altLabel xml:lang="en">Document categorisation</skos:altLabel>
        <skos:altLabel xml:lang="en-uk">Document categorisation</skos:altLabel>
        <skos:altLabel xml:lang="en">Document categorization</skos:altLabel>
