        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources"/>
        <rdfs:comment xml:lang="en">The task/process of constructing lexical resources from corpora</rdfs:comment>
        <rdfs:label xml:lang="en">Lexicon acquisition from corpora</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/LexiconCreation -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/LexiconCreation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources"/>
        <rdfs:comment xml:lang="en">The task/process of constructing lexical resources (e.g. dictionaries, computational lexica, glossaries, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">Lexicon creation</rdfs:label>
        <skos:altLabel xml:lang="en">Lexicon Development</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/LexiconEnhancement -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/LexiconEnhancement">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources"/>
        <rdfs:comment xml:lang="en">The task/process of improving (e.g. increasing the size of entries, improving the information, adding new types of information, etc.) a lexicon</rdfs:comment>
        <rdfs:label xml:lang="en">Lexicon enhancement</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/LexiconExtractionFromLexica -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/LexiconExtractionFromLexica">
