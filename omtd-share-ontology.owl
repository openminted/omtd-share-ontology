        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SupportComponent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/performsOperation"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/meta-share/omtd-share/Evaluation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A component that is used in the evaluation of the performance of a component</rdfs:comment>
        <rdfs:label xml:lang="en">Evaluator</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/Event -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/Event">
