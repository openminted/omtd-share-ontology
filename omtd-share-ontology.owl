            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A thing that happens or takes place, especially one of importance [https://en.oxforddictionaries.com/definition/event]</rdfs:comment>
        <rdfs:label xml:lang="en">Event</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EventDetection -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EventDetection">
