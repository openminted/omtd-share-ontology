    <rdfs:label xml:lang="en">SGML</rdfs:label>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/DataFormatScheme"/>
    <skos:prefLabel xml:lang="en">SGML</skos:prefLabel>
