    <rdfs:label xml:lang="en">Human Computer Interaction</rdfs:label>
    <skos:altLabel xml:lang="en">HCI</skos:altLabel>
    <skos:altLabel xml:lang="en">Human Machine Interaction</skos:altLabel>
    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/AvatarSynthesis"/>
    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/ConversationalSystemsBuilding"/>
