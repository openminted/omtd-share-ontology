        <rdfs:comment xml:lang="en">The task/process of attaching tags to all the mentions of an entity in a text linking them together with the entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Entity linking</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EntityMentionLabelling -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EntityMentionLabelling">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/InformationExtraction"/>
        <rdfs:label xml:lang="en">Entity mention labelling</rdfs:label>
        <skos:altLabel xml:lang="en-us">Entity mention labeling [en-US]</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EntityMentionPair -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EntityMentionPair">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/Coreference"/>
