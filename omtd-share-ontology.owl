    <terms:creator>Petr Knoth</terms:creator>
    <terms:creator>Sophie Aubin</terms:creator>
    <terms:issued>2019-05-15</terms:issued>
    <terms:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
    <vann:preferredNamespacePrefix>omtd</vann:preferredNamespacePrefix>
    <owl:priorVersion>1.1.0</owl:priorVersion>
    <owl:versionInfo>pre-release 2.0.0</owl:versionInfo>
  </rdf:Description>
