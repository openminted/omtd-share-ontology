    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/MsPowerpoint -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/MsPowerpoint">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/DocumentFormat"/>
        <rdfs:subClassOf>
