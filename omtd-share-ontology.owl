  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/StructuralAnnotation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Structural annotation</rdfs:label>
