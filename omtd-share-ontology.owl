  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/Summarization">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Summarization</rdfs:label>
    <skos:altLabel xml:lang="en">ATS</skos:altLabel>
    <skos:altLabel xml:lang="en-uk">Automatic Summarisation</skos:altLabel>
