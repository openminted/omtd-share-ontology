    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/AnnotationTypeScheme"/>
    <skos:prefLabel xml:lang="en">Theoretical frame</skos:prefLabel>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/NamedEntity"/>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/SocialSciencesEntity"/>
  </rdf:Description>
