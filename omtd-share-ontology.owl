        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/ScholarlyCommunicationAnnotation"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SemanticAnnotationType"/>
        <rdfs:comment xml:lang="en">A word or group of words used to describe or index the contents of a document</rdfs:comment>
        <rdfs:label xml:lang="en">Keyword</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/KeywordExtraction -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/KeywordExtraction">
