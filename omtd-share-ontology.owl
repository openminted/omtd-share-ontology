    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/Transcription"/>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
    <skos:prefLabel xml:lang="en">Speech Recognition</skos:prefLabel>
