    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/AnnotationOfDocumentStructure"/>
    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/ParagraphSplitting"/>
    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/SentenceSplitting"/>
    <skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/Tokenization"/>
