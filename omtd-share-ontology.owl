        <rdfs:label xml:lang="en">Morphological analysis</rdfs:label>
        <skos:altLabel xml:lang="en">General morphological analyisis</skos:altLabel>
        <skos:definition xml:lang="en">The technologies for or the process of tracing the inflectional, derivational, and compounding processes in the formation of a given word in order to determine properties such as stem form, part-of-speech and inflectional information. As a crucial preprocessing step, morphological analysis is used in virtually all fields of natural language processing.</skos:definition>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/TextAnnotation"/>
        <rdfs:comment xml:lang="en">The task/process of adding annotations pertaining to the morphological level of analysis (e.g. gender, number, person etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">Morphological annotation</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/MorphologicalAnnotationType -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotationType">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/AnnotationType"/>
        <rdfs:comment xml:lang="en">Any type of annotation pertaining to the morphological level</rdfs:comment>
        <rdfs:label xml:lang="en">Morphological annotation type</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/MorphologicalFeature -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/MorphologicalFeature">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotationType"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2002/07/owl#equivalentClass"/>
