    <skos:prefLabel xml:lang="en">CoNLL-2002</skos:prefLabel>
    <skos:definition xml:lang="en">The CoNLL 2002 format encodes named entity spans. Fields are separated by a single space. Sentences are separated by a blank new line.</skos:definition>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/ConllFormat"/>
    <hasMimetype rdf:resource="http://w3id.org/meta-share/omtd-share/text_x_org_dkpro_conll2002"/>
