        <rdfs:comment xml:lang="en">A component that detects and annotates equivalence relations between items (corpora, texts, paragraphs, sentences, phrases, words) in two languages</rdfs:comment>
        <rdfs:label xml:lang="en">Aligner</rdfs:label>
    </owl:Class>
    


