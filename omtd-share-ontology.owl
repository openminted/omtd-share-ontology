    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/MsWord -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/MsWord">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/DocumentFormat"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/hasMimetype"/>
                <owl:hasValue rdf:resource="http://w3id.org/meta-share/omtd-share/application_msword"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
