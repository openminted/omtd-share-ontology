  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/KernelMethod">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dc:source xml:lang="en">adapted from wikipedia (https://en.wikipedia.org/wiki/Kernel_method)</dc:source>
    <rdfs:label xml:lang="en">Kernel Method</rdfs:label>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/TdmMethodScheme"/>
    <skos:prefLabel xml:lang="en">Kernel Method</skos:prefLabel>
    <skos:definition xml:lang="en">Any method used in pattern analysis that relies on kernel functions, which enable it to operate in a high-dimensional, implicit feature space without ever computing the coordinates of the data in that space, but rather by simply computing the inner products between the images of all pairs of data in the feature space.</skos:definition>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/Brat">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">BRAT</rdfs:label>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/DataFormatScheme"/>
