    <skos:prefLabel xml:lang="en">Multilingual Information Retrieval</skos:prefLabel>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/InformationRetrieval"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/MorphologicalAnalysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Morphological analysis</rdfs:label>
    <skos:altLabel xml:lang="en">General morphological analyisis</skos:altLabel>
    <skos:definition xml:lang="en">The technologies for or the process of tracing the inflectional, derivational, and compounding processes in the formation of a given word in order to determine properties such as stem form, part-of-speech and inflectional information. As a crucial preprocessing step, morphological analysis is used in virtually all fields of natural language processing.</skos:definition>
    <skos:definition xml:lang="en">The analysis of the structure of words and their relations to other words as regards their form and derivation</skos:definition>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
    <skos:prefLabel xml:lang="en">Morphological analysis</skos:prefLabel>
    <skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/LinguisticAnalysis"/>
  </rdf:Description>
