        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/hasMimetype"/>
                <owl:hasValue rdf:resource="http://w3id.org/meta-share/omtd-share/application_xLatex"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/documentationUrl"/>
                <owl:hasValue>https://www.latex-project.org/about/</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Data format for documents using LaTeX (a high-quality typesetting system very popular for scientific documents)</rdfs:comment>
        <rdfs:label xml:lang="en">LATEX</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/Lemma -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/Lemma">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotationType"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/documentationUrl"/>
                <owl:hasValue>http://dkpro.github.io/dkpro-core/releases/1.8.0/docs/typesystem-reference.html#de.tudarmstadt.ukp.dkpro.core.api.segmentation.type.Lemma</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The canonical or citation form used for referring to a word and its inflected forms</rdfs:comment>
        <rdfs:label xml:lang="en">Lemma</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/Lemmatization -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/Lemmatization">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/TextAnnotation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#closeMatch"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/meta-share/omtd-share/Stemming"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Lemmatisation  (or lemmatization) in linguistics is the process of grouping together the inflected forms of a word so they can be analysed as a single item, identified by the word&apos;s lemma, or dictionary form. [Wikipedia]</rdfs:comment>
        <rdfs:label xml:lang="en">Lemmatization</rdfs:label>
        <skos:altLabel xml:lang="en-uk">Lemmatisation</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/Lemmatizer -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/Lemmatizer">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/Annotator"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/performsOperation"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/meta-share/omtd-share/Lemmatization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A component that annotates the tokens of a text with lemma information</rdfs:comment>
        <rdfs:label xml:lang="en">Lemmatizer</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/LexicalSemanticRelation -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/LexicalSemanticRelation">
