        <rdfs:comment xml:lang="en">The task of computing and extracting quantitative properties (e.g. of strings, words, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">Extraction of quantitative information</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/FaceDetection -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/FaceDetection">
