        <rdfs:label xml:lang="en">Facial expression recognition</rdfs:label>
        <skos:altLabel xml:lang="en">Expression detection</skos:altLabel>
        <skos:altLabel xml:lang="en">Expression recognition</skos:altLabel>
        <skos:altLabel xml:lang="en">Face expression detection</skos:altLabel>
        <skos:altLabel xml:lang="en">Face expression recognition</skos:altLabel>
        <skos:altLabel xml:lang="en">Facial expression detection</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/Facility -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/Facility">
