        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources"/>
        <rdfs:comment xml:lang="en">The task/process of constructing lexical resources based on the restructuring of lexical information contained in lexica (e.g. by parsing definitions or using syntactic information attached to lemmas)</rdfs:comment>
        <rdfs:label xml:lang="en">Lexicon extraction from lexica</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/LexiconExtractorFromCorpora -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/LexiconExtractorFromCorpora">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/Analyzer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/meta-share/omtd-share/performsOperation"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/meta-share/omtd-share/LexiconAcquisitionFromCorpora"/>
