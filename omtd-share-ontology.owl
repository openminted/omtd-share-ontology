                <owl:hasValue>https://builds.openminted.eu/job/WP%205.2%20-%20Typesystem%20alignment/eu.openminted.interop$mapping-conversion/doclinks/1/components.html#_factored_tag_lem_1</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Factored tag lemma format</rdfs:comment>
        <rdfs:label xml:lang="en">Factored tag lem format</rdfs:label>
    </owl:Class>
    


