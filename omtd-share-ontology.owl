        <rdfs:comment xml:lang="en">The process/task of identifying events in data (text, video, images etc.), usually combined with their classification into types of events and recognition of the event attributes (e.g. time, place, participants and duration)</rdfs:comment>
        <rdfs:label xml:lang="en">Event detection</rdfs:label>
        <skos:altLabel xml:lang="en">Event extraction</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EventExtractor -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EventExtractor">
