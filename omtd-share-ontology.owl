        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/NamedEntity"/>
        <rdfs:comment xml:lang="en">A set of animals or plants in which the members have similar characteristics to each other and can breed with each other</rdfs:comment>
        <rdfs:label xml:lang="en">Species</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/SpectralData -->

