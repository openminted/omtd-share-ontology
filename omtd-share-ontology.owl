        <rdfs:label xml:lang="en">Emotion recognition</rdfs:label>
        <skos:altLabel xml:lang="en">Emotion detection</skos:altLabel>
        <skos:definition xml:lang="en">The recognition of emotions from text, speech, facial expressions, gestures and/or physiological measures. A key challenge is the appropriate representation of emotional states.</skos:definition>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EmotionGeneration -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EmotionGeneration">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/OperationWithMultimediaInputOrOutput"/>
        <rdfs:label xml:lang="en">Emotion generation</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EmotionLabelling -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EmotionLabelling">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/InformationExtraction"/>
        <rdfs:comment xml:lang="en">The process/task of identifying types of feelings (e.g. anger, fear, happiness, sadness, etc.) in the linguistic expression of texts or facial expressions</rdfs:comment>
        <rdfs:label xml:lang="en">Emotion labelling</rdfs:label>
        <skos:altLabel xml:lang="en-us">Emotion labeling [en-US]</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/EmotionRecognizer -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/EmotionRecognizer">
