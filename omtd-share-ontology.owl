        <rdfs:label xml:lang="en">Face recognition</rdfs:label>
        <skos:altLabel xml:lang="en">Facial recognition</skos:altLabel>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/FaceVerification -->

