    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/AgriculturalEntity">
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/meta-share/omtd-share/DomainSpecificAnnotationType"/>
        <rdfs:comment xml:lang="en">Any kind of annotation pertaining to entities of the agricultural domain; the use of the AGROVOC thesaurus is recommended</rdfs:comment>
        <rdfs:label xml:lang="en">Agricultural entity</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/meta-share/omtd-share/AimedCorpusFormat -->

    <owl:Class rdf:about="http://w3id.org/meta-share/omtd-share/AimedCorpusFormat">
