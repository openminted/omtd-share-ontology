  <rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/EnsembleMethod">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dc:source xml:lang="en">adapted from Wikipedia (https://en.wikipedia.org/wiki/Ensemble_learning)</dc:source>
    <rdfs:label xml:lang="en">Ensemble Method</rdfs:label>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/TdmMethodScheme"/>
    <skos:prefLabel xml:lang="en">Ensemble Method</skos:prefLabel>
    <skos:definition xml:lang="en">Any method that uses multiple learning algorithms in an attempt to improve predictive performance not obtainable otherwise with any of the constituent learning algorithms.</skos:definition>
