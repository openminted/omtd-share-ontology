    <dc:source xml:lang="en">adapted from Wikipedia (https://en.wikipedia.org/wiki/Bayesian_inference)</dc:source>
    <rdfs:label xml:lang="en">Bayesian</rdfs:label>
    <skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/TdmMethodScheme"/>
    <skos:prefLabel xml:lang="en">Bayesian</skos:prefLabel>
