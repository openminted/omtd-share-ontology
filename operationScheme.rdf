<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/LanguageChecking">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Language checking</rdfs:label>
	<skos:altLabel xml:lang="en">Proofing</skos:altLabel>
	<skos:definition xml:lang="en">Language Checking comprises technologies used to detect and/or correct erroneous or inconsistent language use in documents. The scope of language checking technology ranges from general error correction, as performed by spell checkers and grammar checkers</skos:definition>
	<skos:definition xml:lang="en">The task/process of identifying (and usually correcting) grammatical mistakes in a text</skos:definition>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Language checking</skos:prefLabel>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/AuthoringSupport"/>
</rdf:Description>

