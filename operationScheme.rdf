	<skos:altLabel xml:lang="en">Chatbots building</skos:altLabel>
	<skos:altLabel xml:lang="en">Conversational agents building</skos:altLabel>
	<skos:altLabel xml:lang="en">Dialogue systems building</skos:altLabel>
	<skos:altLabel xml:lang="en">Embodied agents building</skos:altLabel>
	<skos:altLabel xml:lang="en">Virtual agents building</skos:altLabel>
