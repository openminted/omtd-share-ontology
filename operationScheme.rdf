	<skos:altLabel xml:lang="en">B-PoS Tagging</skos:altLabel>
	<skos:altLabel xml:lang="en">Below PoS Tagging</skos:altLabel>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Below Part-of-Speech Tagging</skos:prefLabel>
	<skos:definition xml:lang="en">The annotation of words with morphological information besides the part of speech and dependent upon it (e.g. for nouns: gender, number and case; for verbs: tense, number, person etc.)</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation"/>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging"/>
</rdf:Description>

