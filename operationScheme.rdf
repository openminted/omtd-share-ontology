	<skos:prefLabel xml:lang="en">Semantic search</skos:prefLabel>
	<skos:definition xml:lang="en">A type of search that seeks to improve search accuracy by understanding the searcher's intent and the contextual meaning of terms as they appear in the searchable dataspace, whether on the Web or within a closed system, to generate more relevant results.</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/InformationRetrieval"/>
</rdf:Description>

<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/FakeNewsDetection">
