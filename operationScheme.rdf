	<skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/SpeechUnderstanding"/>
	<skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/SpokenLanguageUnderstanding"/>
