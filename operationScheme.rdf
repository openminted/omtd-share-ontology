<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/FactChecking">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Fact checking</rdfs:label>
	<skos:altLabel xml:lang="en">Fact-checking</skos:altLabel>
	<skos:related rdf:resource="http://w3id.org/meta-share/omtd-share/FakeNewsDetection"/>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Fact checking</skos:prefLabel>
	<skos:definition xml:lang="en">The act of checking factual assertions in non-fictional text in order to determine the veracity and correctness of the factual statements in the text [from Wikipedia]</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/InformationExtraction"/>
</rdf:Description>

<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/ParallelSentenceExtraction">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Parallel sentence extraction</rdfs:label>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Parallel sentence extraction</skos:prefLabel>
	<skos:definition xml:lang="en">The task/process of identifying parallel sentences (i.e. sentences that have the same sense in different languages) in parallel texts</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MachineTranslationSupport"/>
</rdf:Description>

<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/AnnotationOfCompoundingFeatures">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Annotation of compounds</rdfs:label>
	<owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Annotation of compounds</skos:prefLabel>
	<skos:definition xml:lang="en">The task/process of marking compounds (single words composed of two or more free morphemes) and their parts</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation"/>
</rdf:Description>

<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/DiscourseAnalysis">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Discourse analysis</rdfs:label>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Discourse analysis</skos:prefLabel>
	<skos:definition xml:lang="en">A method of analysing the structure of texts or utterances longer than one sentence, taking into account both their linguistic content and their sociolinguistic context; analysis performed using this method.[OED, https://en.oxforddictionaries.com/definition/discourse_analysis]</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/LinguisticAnalysis"/>
</rdf:Description>

<rdf:Description rdf:about="http://w3id.org/meta-share/omtd-share/NamedEntityRecognition">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="en">Named Entity Recognition</rdfs:label>
	<skos:altLabel xml:lang="en">Entity Identification</skos:altLabel>
	<skos:altLabel xml:lang="en">Entity extraction</skos:altLabel>
	<skos:altLabel xml:lang="en">Entity Recognition</skos:altLabel>
	<skos:altLabel xml:lang="en">NER</skos:altLabel>
	<skos:altLabel xml:lang="en">NERC</skos:altLabel>
	<skos:definition xml:lang="en">Named entity (NE) recognition is a form of information extraction in which the major task is to identify and classify from NL text every word or sequence of words as being a person-name, organizaton, location, date, time, monetary value, percentage expression. NE recognition has a high impact for a number of applications, like e.g., InterNet search engines, text data mining or answer extraction.</skos:definition>
	<skos:definition xml:lang="en">A subtask of information extraction that seeks to locate and classify named entities in text into pre-defined categories such as the names of persons, organizations, locations, expressions of times, quantities, monetary values, percentages, etc.</skos:definition>
	<skos:editorialNote xml:lang="en">can also be annotation</skos:editorialNote>
	<skos:narrower rdf:resource="http://w3id.org/meta-share/omtd-share/NamedEntityDisambiguation"/>
	<skos:inScheme rdf:resource="http://w3id.org/meta-share/omtd-share/OperationScheme"/>
	<skos:prefLabel xml:lang="en">Named Entity Recognition</skos:prefLabel>
</rdf:Description>

