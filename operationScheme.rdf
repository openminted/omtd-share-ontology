	<skos:definition xml:lang="en">The task/process of annotating multi-word units, i.e. combinations of words that are considered as one</skos:definition>
	<skos:broader rdf:resource="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation"/>
</rdf:Description>

